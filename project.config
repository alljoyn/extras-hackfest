[access]
	inheritFrom = All-Projects
[project]
	description = Hackfest examples and support code
[access "refs/for/refs/heads/*"]
	push = group extra-hackfest
	push = group ldap/hackfest
[access "refs/heads/*"]
	create = group extra-hackfest
	create = group ldap/hackfest
	forgeAuthor = group extra-hackfest
	forgeAuthor = group ldap/hackfest
	label-Code-Review = -2..+2 group extra-hackfest
	label-Code-Review = -2..+2 group ldap/hackfest
	submit = group extra-hackfest
	submit = group ldap/hackfest
	editTopicName = group extra-hackfest
	editTopicName = group ldap/hackfest
	abandon = group extra-hackfest
	abandon = group ldap/hackfest
[access "refs/meta/config"]
	read = group extra-hackfest
	read = group ldap/hackfest
[access "refs/tags/*"]
	pushTag = group extra-hackfest
	pushTag = group ldap/hackfest
	pushSignedTag = group extra-hackfest
	pushSignedTag = group ldap/hackfest
[access "refs/for/*"]
	pushMerge = group extra-hackfest
	pushMerge = group ldap/hackfest
[access "refs/*"]
	read = group GitHub Replication
